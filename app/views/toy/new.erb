<h1>New Toy!</h1>
<form action="/toys" method="POST">
  <label for="name">Name</label>
  <input type="text" name="name">
  <label for="dog">Dog</label>
  <select name="dog">
    <% Dog.all.each do |dog| %>
    <option value="<%= dog.id %>"><%= dog.name %></option>
    <% end %>
  </select>
<input type="submit">

  </form>
